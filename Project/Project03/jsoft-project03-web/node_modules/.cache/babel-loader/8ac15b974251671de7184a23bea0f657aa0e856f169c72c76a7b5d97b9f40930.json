<a href=\"javascript:void(0)\" @click=\"goToLastPage()\" class=\"fy1\">尾页</a>\n            </div>\n            <div class=\"clear\"></div>\n        </div>\n        <div class=\"clear\"></div>\n        <div class=\"footer\">\n        </div>\n    </div>\n</template>\n<script>\n// edit by JoneElmo 10-29\nimport axios from \"axios\"\nexport default {\n    name: 'indexTopBar',\n    data() {\n        return {\n            arr: new Array(10),\n            isOn: true,\n            isNone: true,\n\n            userName: null,\n\n            loginhref: \"#\",\n\n            word: \"\",\n\n            result: null,\n\n            recruitList: null,\n\n            //总数据条数\n            total: null,\n            //总页数\n            pages: null,\n            //前一页\n            prePage: null,\n            //后一页\n            nextPage: null,\n            //当前页\n            pageNum: null,\n            //页面数据量\n            pageSize: null\n        }\n    },\n    methods: {    \n        \n        // 跳转到第一页\n        goToFirstPage() {\n            this.goToPage(1);\n        },\n        // 跳转到上一页\n        goToPreviousPage() {\n            if (this.currentPage > 1) {\n                this.goToPage(this.currentPage - 1);\n            }\n        },\n        // 跳转到下一页\n        goToNextPage() {\n            if (this.currentPage < this.totalPages) {\n                this.goToPage(this.currentPage + 1);\n            }\n        },\n        // 跳转到最后一页\n        goToLastPage() {\n            this.goToPage(this.totalPages);\n        },\n        goToPage(page) {\n            this.pageNum = page\n        },\n        setPageInfo() {\n            console.log(\"接收到的数据信息：\")\n            console.log(this.result.data.object)\n            console.log(\"接收到的列表信息：\")\n            console.log(this.result.data.object.list)\n            this.recruitList = this.result.data.object.list\n            //注入分页信息\n            this.total = this.result.data.object.total\n            this.pages = this.result.data.object.pages\n            this.prePage = this.result.data.object.prePage\n            this.nextPage = this.result.data.object.nextPage\n            this.pageNum = this.result.data.object.pageNum\n            this.pageSize = this.result.data.object.pageSize\n        },\n        logOut() {\n            // 获取所有Cookie的名称\n            const cookieNames = Object.keys(this.$cookie.get());\n            // 清空所有Cookie\n            cookieNames.forEach(cookieName => {\n                this.$cookie.remove(cookieName);\n            });\n            location.reload()\n        },\n        showOn(status) {\n            if (status == 'list') {\n                this.isOn = true,\n                    this.isNone = true\n            } else {\n                this.isOn = false,\n                    this.isNone = false\n            }\n        }\n    },\n    mounted() {\n        this.word = this.$route.query.word\n        console.log(\"接收到的关键字:\" + this.word)\n        const userName = this.$cookie.get(\"cookieUserName\")\n        // console.log(userName+\";\"+userId+\";\"+userEmail+\";\"+userPhone)\n        console.log(this.$cookie.get())\n        if (userName != null && userName != \"\") {\n            console.log(\"用户已登陆\")\n            this.userName = userName\n            this.loginhref = \"/personal/resume\"\n        } else {\n            this.userName = \"个人登陆\"\n            this.loginhref = \"/login\"\n        }\n\n        // 模糊查询发送请求\n        if (this.word == null || this.word == \"\") {\n            let that = this\n            //获取全部数据\n            axios({\n                method: 'get',\n                url: '/api/recruit/queryRecruitList'\n            })\n                .then(function (result) {\n                    that.result = result\n                    that.setPageInfo()\n                })\n        } else {\n            let that = this\n            //根据关键字获取数据\n            axios({\n                method: 'get',\n                url: '/api/recruit/queryRecruitList?word=' + that.word\n            })\n                .then(function (result) {\n                    that.result = result\n                    that.setPageInfo()\n                })\n        }\n\n    }\n}\n</script>\n<style>\n.loginHrefClass {\n    margin-right: 50px;\n}\n</style>"],"mappings":";;;;AAwHA;AACA,OAAAA,KAAA;AACA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,GAAA,MAAAC,KAAA;MACAC,IAAA;MACAC,MAAA;MAEAC,QAAA;MAEAC,SAAA;MAEAC,IAAA;MAEAC,MAAA;MAEAC,WAAA;MAEA;MACAC,KAAA;MACA;MACAC,KAAA;MACA;MACAC,OAAA;MACA;MACAC,QAAA;MACA;MACAC,OAAA;MACA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IAEA;IACAC,aAAA,WAAAA,cAAA;MACA,KAAAC,QAAA;IACA;IACA;IACAC,gBAAA,WAAAA,iBAAA;MACA,SAAAC,WAAA;QACA,KAAAF,QAAA,MAAAE,WAAA;MACA;IACA;IACA;IACAC,YAAA,WAAAA,aAAA;MACA,SAAAD,WAAA,QAAAE,UAAA;QACA,KAAAJ,QAAA,MAAAE,WAAA;MACA;IACA;IACA;IACAG,YAAA,WAAAA,aAAA;MACA,KAAAL,QAAA,MAAAI,UAAA;IACA;IACAJ,QAAA,WAAAA,SAAAM,IAAA;MACA,KAAAV,OAAA,GAAAU,IAAA;IACA;IACAC,WAAA,WAAAA,YAAA;MACAC,OAAA,CAAAC,GAAA;MACAD,OAAA,CAAAC,GAAA,MAAAnB,MAAA,CAAAR,IAAA,CAAA4B,MAAA;MACAF,OAAA,CAAAC,GAAA;MACAD,OAAA,CAAAC,GAAA,MAAAnB,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAC,IAAA;MACA,KAAApB,WAAA,QAAAD,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAC,IAAA;MACA;MACA,KAAAnB,KAAA,QAAAF,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAlB,KAAA;MACA,KAAAC,KAAA,QAAAH,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAjB,KAAA;MACA,KAAAC,OAAA,QAAAJ,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAhB,OAAA;MACA,KAAAC,QAAA,QAAAL,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAf,QAAA;MACA,KAAAC,OAAA,QAAAN,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAd,OAAA;MACA,KAAAC,QAAA,QAAAP,MAAA,CAAAR,IAAA,CAAA4B,MAAA,CAAAb,QAAA;IACA;IACAe,MAAA,WAAAA,OAAA;MAAA,IAAAC,KAAA;MACA;MACA,IAAAC,WAAA,GAAAC,MAAA,CAAAC,IAAA,MAAAC,OAAA,CAAAC,GAAA;MACA;MACAJ,WAAA,CAAAK,OAAA,WAAAC,UAAA;QACAP,KAAA,CAAAI,OAAA,CAAAI,MAAA,CAAAD,UAAA;MACA;MACAE,QAAA,CAAAC,MAAA;IACA;IACAC,MAAA,WAAAA,OAAAC,MAAA;MACA,IAAAA,MAAA;QACA,KAAAxC,IAAA,SACA,KAAAC,MAAA;MACA;QACA,KAAAD,IAAA,UACA,KAAAC,MAAA;MACA;IACA;EACA;EACAwC,OAAA,WAAAA,QAAA;IACA,KAAArC,IAAA,QAAAsC,MAAA,CAAAC,KAAA,CAAAvC,IAAA;IACAmB,OAAA,CAAAC,GAAA,mBAAApB,IAAA;IACA,IAAAF