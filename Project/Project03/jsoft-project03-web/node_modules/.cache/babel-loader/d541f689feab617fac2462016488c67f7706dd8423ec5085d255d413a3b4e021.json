geInfo()\n                    })\n            }\n        },\n        // 跳转到第一页\n        goToFirstPage() {\n            this.goToPage(1);\n        },\n        // 跳转到上一页\n        goToPreviousPage() {\n            if (this.currentPage > 1) {\n                this.goToPage(this.currentPage - 1);\n            }\n        },\n        // 跳转到下一页\n        goToNextPage() {\n            if (this.currentPage < this.totalPages) {\n                this.goToPage(this.currentPage + 1);\n            }\n        },\n        // 跳转到最后一页\n        asgoToLastPage() {\n            this.goToPage(this.totalPages);\n        },\n        async goToPage(page) {\n            this.pageNum = page\n            await this.loadDataForPage(this.pageNum)\n        },\n        setPageInfo() {\n            console.log(\"接收到的数据信息：\")\n            console.log(this.result.data.object)\n            console.log(\"接收到的列表信息：\")\n            console.log(this.result.data.object.list)\n            this.recruitList = this.result.data.object.list\n            //注入分页信息\n            this.total = this.result.data.object.total\n            this.pages = this.result.data.object.pages\n            this.prePage = this.result.data.object.prePage\n            this.nextPage = this.result.data.object.nextPage\n            this.pageSize = this.result.data.object.pageSize\n        },\n        logOut() {\n            // 获取所有Cookie的名称\n            const cookieNames = Object.keys(this.$cookie.get());\n            // 清空所有Cookie\n            cookieNames.forEach(cookieName => {\n                this.$cookie.remove(cookieName);\n            });\n            location.reload()\n        },\n        showOn(status) {\n            if (status == 'list') {\n                this.isOn = true,\n                    this.isNone = true\n            } else {\n                this.isOn = false,\n                    this.isNone = false\n            }\n        }\n    },\n    mounted() {\n        this.word = this.$route.query.word\n        console.log(\"接收到的关键字:\" + this.word)\n        const userName = this.$cookie.get(\"cookieUserName\")\n        // console.log(userName+\";\"+userId+\";\"+userEmail+\";\"+userPhone)\n        console.log(this.$cookie.get())\n        if (userName != null && userName != \"\") {\n            console.log(\"用户已登陆\")\n            this.userName = userName\n            this.loginhref = \"/personal/resume\"\n        } else {\n            this.userName = \"个人登陆\"\n            this.loginhref = \"/login\"\n        }\n\n        this.query()\n\n    }\n}\n</script>\n<style>\n.loginHrefClass {\n    margin-right: 50px;\n}\n</style>"],"mappings":";;;;;;AAuHA;AACA,OAAAA,KAAA;AACA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,GAAA,MAAAC,KAAA;MACAC,IAAA;MACAC,MAAA;MAEAC,QAAA;MAEAC,SAAA;MAEAC,IAAA;MAEAC,MAAA;MAEAC,WAAA;MAEA;MACAC,KAAA;MACA;MACAC,KAAA;MACA;MACAC,OAAA;MACA;MACAC,QAAA;MACA;MACAC,OAAA;MACA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA,WAAAA,gBAAA;MAAA,IAAAC,KAAA;MAAA,OAAAC,iBAAA,eAAAC,mBAAA,GAAAC,IAAA,UAAAC,QAAA;QAAA,IAAAC,IAAA;QAAA,OAAAH,mBAAA,GAAAI,IAAA,UAAAC,SAAAC,QAAA;UAAA,kBAAAA,QAAA,CAAAC,IAAA,GAAAD,QAAA,CAAAE,IAAA;YAAA;cACAL,IAAA,GAAAL,KAAA,EACA;cACApB,KAAA;gBACA+B,MAAA;gBACAC,GAAA,0CAAAZ,KAAA,CAAAX,IAAA,iBAAAW,KAAA,CAAAJ;cACA,GACAiB,IAAA,WAAAvB,MAAA;gBACAe,IAAA,CAAAf,MAAA,GAAAA,MAAA;gBACAe,IAAA,CAAAS,WAAA;cACA;YAAA;YAAA;cAAA,OAAAN,QAAA,CAAAO,IAAA;UAAA;QAAA,GAAAX,OAAA;MAAA;IACA;IACAY,KAAA,WAAAA,MAAA;MACA;MACA,SAAA3B,IAAA,iBAAAA,IAAA;QACA,IAAAgB,IAAA;QACA;QACAzB,KAAA;UACA+B,MAAA;UACAC,GAAA;QACA,GACAC,IAAA,WAAAvB,MAAA;UACAe,IAAA,CAAAf,MAAA,GAAAA,MAAA;UACAe,IAAA,CAAAS,WAAA;QACA;MACA;QACA,IAAAT,KAAA;QACA;QACAzB,KAAA;UACA+B,MAAA;UACAC,GAAA,0CAAAP,KAAA,CAAAhB;QACA,GACAwB,IAAA,WAAAvB,MAAA;UACAe,KAAA,CAAAf,MAAA,GAAAA,MAAA;UACAe,KAAA,CAAAS,WAAA;QACA;MACA;IACA;IACA;IACAG,aAAA,WAAAA,cAAA;MACA,KAAAC,QAAA;IACA;IACA;IACAC,gBAAA,WAAAA,iBAAA;MACA,SAAAC,WAAA;QACA,KAAAF,QAAA,MAAAE,WAAA;MACA;IACA;IACA;IACAC,YAAA,WAAAA,aAAA;MACA,SAAAD,WAAA,QAAAE,UAAA;QACA,KAAAJ,QAAA,MAAAE,WAAA;MACA;IACA;IACA;IACAG,cAAA,WAAAA,eAAA;