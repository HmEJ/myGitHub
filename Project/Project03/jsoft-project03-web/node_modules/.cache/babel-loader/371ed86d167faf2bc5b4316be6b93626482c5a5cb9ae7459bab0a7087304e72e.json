                this.show1 = true\n            }\n            if (i == 2) {\n                this.show2 = true\n            }\n            if (i == 3) {\n                this.show3 = true\n            }\n        }\n    },\n    mounted() {\n        let that = this;\n        axios({\n            method: 'GET',\n            url: '/api/user-info/getPhoneOrEmail/100001' //你的后端路径\n        })\n            .then(response => {\n                let data = response.data\n                let code = data.code\n                let msg = data.msg\n                if (code === 200001) { //判断你的请求是否成功\n                    console.log(data)\n                    if (data.object.phone != \"\") {\n                        that.phone = data.object.phone\n                        that.phoneReadonly = true\n                    }else{\n                        that.email = data.object.email\n                        that.emailReadonly = true\n                    }\n                } else {\n                    alert(msg)\n                }\n            }, error => {\n                console.log('错误', error.message)\n                alert(error.message)\n            })\n    }\n}\n</script>"],"mappings":"AAgQA,OAAAA,KAAA;AACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,SAAA;MACAC,cAAA;MACAC,QAAA;MACAC,QAAA;MACAC,KAAA;MACAC,KAAA;MACAC,KAAA;MACAC,QAAA;MAAA;MACAC,MAAA;MAAA;MACAC,QAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MAAA;MACAC,SAAA;MAAA;MACAC,KAAA;MAAA;MACAC,KAAA;MAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,YAAA;MAAA;MACAC,YAAA;MAAA;MACAC,cAAA;MAAA;MACAC,UAAA;MAAA;MACAC,UAAA;MAAA;MACAC,cAAA;MAAA;MACAC,MAAA;MAAA;MACAC,UAAA;MAAA;MACAC,SAAA;MAAA;MACAC,SAAA;MAAA;MACAC,aAAA;MACAC,aAAA;IAIA;EACA;EACAC,OAAA;IACAC,IAAA,WAAAA,KAAA;MACAC,KAAA,MAAAb,YAAA;MACA,KAAAlB,SAAA,SAAAA,SAAA;MACA,KAAAC,cAAA,SAAAA,cAAA;IACA;IACA+B,OAAA,WAAAA,QAAAC,GAAA;MACA,IAAAA,GAAA;QACA,KAAA/B,QAAA;MACA;QACA,KAAAA,QAAA;MACA;IACA;IACAgC,OAAA,WAAAA,QAAAC,MAAA;MACA,IAAAA,MAAA;QACA,KAAAhC,QAAA;MACA;QACA,KAAAA,QAAA;MACA;IACA;IACAiC,YAAA,WAAAA,aAAAC,CAAA;