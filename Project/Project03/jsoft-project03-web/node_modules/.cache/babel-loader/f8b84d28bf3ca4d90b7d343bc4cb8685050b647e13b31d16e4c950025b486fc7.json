{"ast":null,"code":"import \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.date.to-string.js\";\nimport \"core-js/modules/es.date.to-iso-string.js\";\nimport \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.regexp.test.js\";\nimport axios from 'axios';\nimport Cookies from 'js-cookie';\nexport default {\n  data: function data() {\n    return {\n      show_base: true,\n      show_education: false,\n      show_sex: true,\n      isActive: true,\n      show1: true,\n      show2: false,\n      show3: false,\n      trueName: \"\",\n      //姓名\n      gender: 1,\n      //性别\n      birthday: \"\",\n      //出生日期\n      resident: \"\",\n      //现居住地\n      place: \"\",\n      //户口所在地\n      growPlace: \"\",\n      //成长所在地\n      phone: \"\",\n      //手机号码\n      email: \"\",\n      //电子邮件\n      jobStatus: 0,\n      //求职状态\n      workExp: 1,\n      //工作经验\n      wantedNature: 0,\n      //期望工作性质\n      wantedSalary: 0,\n      //期望月薪\n      wantedLocation: \"\",\n      //期望工作地点\n      wantedType: \"\",\n      //期望职位类别\n      schoolName: \"\",\n      //学校名称\n      graduationTime: \"\",\n      //毕业时间\n      degree: 0,\n      //学历\n      degreeType: 0,\n      //学历类型\n      majorName: \"\",\n      //专业名称\n      majorType: 0,\n      //专业类别\n      /* 状态中false为只读状态,不显示提示信息 */\n      phoneStatus: false,\n      phoneInfo: \"可用来登录\",\n      emailStatus: false,\n      emailInfo: \"可用来登录\",\n      isRed: false,\n      isDisabled: false,\n      showMan: false,\n      showWoman: false,\n      showHave: false,\n      showNot: false,\n      showFull: false,\n      showPart: false,\n      showPractice: false\n    };\n  },\n  methods: {\n    show: function show() {\n      this.show_base = !this.show_base;\n      this.show_education = !this.show_education;\n    },\n    showSex: function showSex(sex) {\n      if (sex == 'man') {\n        this.show_sex = true;\n      } else {\n        this.show_sex = false;\n      }\n    },\n    showJob: function showJob(status) {\n      if (status == 'yes') {\n        this.isActive = true;\n      } else {\n        this.isActive = false;\n      }\n    },\n    showThreeJob: function showThreeJob(i) {\n      this.show1 = false, this.show2 = false, this.show3 = false;\n      if (i == 1) {\n        this.show1 = true;\n      }\n      if (i == 2) {\n        this.show2 = true;\n      }\n      if (i == 3) {\n        this.show3 = true;\n      }\n    },\n    /* 数据回显 */echoData: function echoData() {\n      var _this = this;\n      var that = this;\n      var cookieValue = Cookies.get('cookieUserId');\n      axios({\n        method: 'GET',\n        url: '/api/user-info/getUserInfo/' + cookieValue\n      }).then(function (response) {\n        var data = response.data;\n        var code = data.code;\n        var msg = data.msg;\n        var info = data.object;\n        if (code === 200001) {\n          /* 从用户信息表获取到数据的话进行数据回显 */\n          console.log(data);\n          _this.trueName = info.name;\n          var backendDate = new Date(info.birthday); // 使用ISO 8601日期字符串创建Date对象\n          backendDate.setDate(backendDate.getDate() + 1);\n          _this.birthday = backendDate.toISOString().split('T')[0];\n          _this.resident = info.resident;\n          _this.place = info.place;\n          _this.growPlace = info.growPlace;\n          _this.phone = info.phone;\n          _this.email = info.email;\n          _this.wantedLocation = info.wantedLocation;\n          _this.wantedType = info.wantedType;\n          _this.schoolName = info.schoolName;\n          _this.majorName = info.majorName;\n          var backendDate2 = new Date(info.graduationTime); // 使用ISO 8601日期字符串创建Date对象\n          backendDate2.setDate(backendDate2.getDate() + 1);\n          _this.graduationTime = backendDate2.toISOString().split('T')[0];\n          _this.isDisabled = true;\n          /* 多选框显示 */\n          if (info.gender == 0) {\n            _this.showMan = false;\n            _this.showWoman = true;\n            _this.show_sex = false;\n          } else {\n            _this.showWoman = false;\n            _this.showMan = true;\n            _this.show_sex = true;\n          }\n          if (info.workExp == 0) {\n            _this.showNot = true;\n            _this.showHave = false;\n            _this.isActive = false;\n          } else {\n            _this.showNot = false;\n            _this.showHave = true;\n            _this.isActive = true;\n          }\n          _this.show1 = false, _this.show2 = false, _this.show3 = false;\n          if (info.wantedNature == 0) {\n            _this.show1 = true;\n            _this.showFull = true;\n            _this.showPart = false;\n            _this.showPractice = false;\n          }\n          if (info.wantedNature == 1) {\n            _this.show2 = true;\n            _this.showFull = false;\n            _this.showPart = true;\n            _this.showPractice = false;\n          }\n          if (info.wantedNature == 2) {\n            _this.show3 = true;\n            _this.showFull = false;\n            _this.showPart = false;\n            _this.showPractice = true;\n          }\n        } else {\n          console.log(msg);\n          _this.showNot = true;\n          _this.showHave = true;\n          _this.showMan = true;\n          _this.showWoman = true;\n          _this.showFull = true;\n          _this.showPart = true;\n          _this.showPractice = true;\n          that.getPhoneOrEmail();\n        }\n      }, function (error) {\n        console.log('错误', error.message);\n      });\n    },\n    getPhoneOrEmail: function getPhoneOrEmail() {\n      var that = this;\n      var cookieValue = Cookies.get('cookieUserId');\n      axios({\n        method: 'GET',\n        url: '/api/user-info/getPhoneOrEmail/' + cookieValue //你的后端路径\n      }).then(function (response) {\n        var data = response.data;\n        var code = data.code;\n        var msg = data.msg;\n        if (code === 200001) {\n          //判断你的请求是否成功\n          console.log(data);\n          if (data.object.phone == \"\") {\n            that.email = data.object.email;\n            that.phoneStatus = true;\n          } else {\n            that.phone = data.object.phone;\n            that.emailStatus = true;\n          }\n        } else {\n          alert(msg);\n        }\n      }, function (error) {\n        console.log('错误', error.message);\n        // alert(error.message)\n      });\n    },\n    addUserInfo: function addUserInfo() {\n      var _this2 = this;\n      var that = this;\n      var cookieValue = Cookies.get('cookieUserId');\n      if (this.trueName != \"\" && this.birthday != \"\" && this.resident != \"\" && this.place != \"\" && this.growPlace != \"\" && this.phone != \"\" && this.email != \"\" && this.wantedLocation != \"\" && this.wantedType != \"\" && this.schoolName != \"\" && this.majorName != \"\") {\n        /* 如果信息填写完整则进行请求发送 */\n        axios({\n          method: 'post',\n          url: '/api/user-info',\n          data: {\n            userId: cookieValue,\n            name: that.trueName,\n            gender: that.gender,\n            birthday: that.birthday,\n            resident: that.resident,\n            place: that.place,\n            growPlace: that.growPlace,\n            phone: that.phone,\n            email: that.email,\n            jobStatus: that.jobStatus,\n            workExp: that.workExp,\n            wantedNature: that.wantedNature,\n            wantedSalary: that.wantedSalary,\n            wantedLocation: that.wantedLocation,\n            wantedType: that.wantedType,\n            schoolName: that.schoolName,\n            graduationTime: that.graduationTime,\n            degree: that.degree,\n            degreeType: that.degreeType,\n            majorName: that.majorName,\n            majorType: that.majorType\n          }\n        }).then(function (response) {\n          var data = response.data;\n          var code = data.code;\n          var msg = data.msg;\n          if (code === 300001) {\n            /* 进行数据回显 */\n            console.log(data.object);\n            alert(msg);\n            _this2.echoData();\n            _this2.show_base = !_this2.show_base;\n            _this2.show_education = !_this2.show_education;\n          } else {\n            alert(msg);\n          }\n        }, function (error) {\n          console.log('错误', error.message);\n          // alert(error.message)\n        });\n      } else {\n        alert(\"请填写完整信息\");\n      }\n    },\n    /* 表单校验 */checkPhone: function checkPhone() {\n      var phone_REG = /^(0\\d{2,3}-\\d{7,8}(-\\d{1,6})?|[1-9]\\d{6,7}(-\\d{1,6})?|1[3-9]\\d{9})$/;\n      if (phone_REG.test(this.phone)) {\n        this.isRed = false;\n        this.phoneInfo = \"可用于登录\";\n      } else {\n        this.isRed = true;\n        this.phoneInfo = \"手机格式不正确\";\n      }\n    },\n    checkEmail: function checkEmail() {\n      var email_REG = /^[a-zA-Z0-9]+@[a-zA-Z0-9]+\\.(com|cn)$/;\n      if (email_REG.test(this.email)) {\n        this.isRed = false;\n        this.emailInfo = \"可用于登录\";\n      } else {\n        this.isRed = true;\n        this.emailInfo = \"邮箱格式不正确\";\n      }\n    }\n  },\n  mounted: function mounted() {\n    this.echoData();\n  }\n};","map":{"version":3,"names":["axios","Cookies","data","show_base","show_education","show_sex","isActive","show1","show2","show3","trueName","gender","birthday","resident","place","growPlace","phone","email","jobStatus","workExp","wantedNature","wantedSalary","wantedLocation","wantedType","schoolName","graduationTime","degree","degreeType","majorName","majorType","phoneStatus","phoneInfo","emailStatus","emailInfo","isRed","isDisabled","showMan","showWoman","showHave","showNot","showFull","showPart","showPractice","methods","show","showSex","sex","showJob","status","showThreeJob","i","echoData","_this","that","cookieValue","get","method","url","then","response","code","msg","info","object","console","log","name","backendDate","Date","setDate","getDate","toISOString","split","backendDate2","getPhoneOrEmail","error","message","alert","addUserInfo","_this2","userId","checkPhone","phone_REG","test","checkEmail","email_REG","mounted"],"sources":["src/pages/page-personal/components/ResumeView.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"personalBody\">\n            <div id=\"base_div\" v-show=\"show_base\" class=\"perRightcon\">\n                <div class=\"JlBoxTit\">\n                    <p class=\"tit1\">基本资料/求职意向</p>\n                    <div class=\"tit2\"></div>\n                    <p class=\"tit3\">最高学历</p>\n                </div>\n                <div class=\"JlBoxCon\">\n                    <h1><span>基本信息</span>\n                        <div></div>\n                    </h1>\n                    <div class=\"JlBoxLeft\">\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>姓 名：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" v-model=\"trueName\" class=\"txt1\" :disabled=\"isDisabled\"/>\n                            </div>\n                            <div class=\"clear\"></div>\n                        </div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>性 别：</div>\n                            <div class=\"inputtxt\">\n                                <label for=\"man\">\n                                    <div class=\"sex\" :class=\"{ on: show_sex }\" @click=\"showSex('man')\" v-if=\"showMan\">男</div>\n                                </label>\n                                <input type=\"radio\" id=\"man\" value=\"1\" v-model=\"gender\" name=\"man\" v-show=\"false\">\n                                <label for=\"woman\">\n                                    <div class=\"sex\" :class=\"{ on: !show_sex }\" @click=\"showSex('girl')\" v-if=\"showWoman\">女</div>\n                                </label>\n                                <input type=\"radio\" id=\"woman\" value=\"0\" v-model=\"gender\" name=\"woman\" v-show=\"false\">\n                                <div class=\"clear\"></div>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>出生年月：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"date\" class=\"txt8\" v-model=\"birthday\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>现居住地：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt7\" v-model=\"resident\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>户口所在地：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt7\" v-model=\"place\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>我成长在：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt7\" v-model=\"growPlace\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>手机号码：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt3\" v-model=\"phone\" @blur=\"checkPhone\"  :readonly=\"!phoneStatus\" :disabled=\"isDisabled\"/>\n                                <b :class=\"{ 'red-text' : isRed}\" v-if=\"phoneStatus\">{{ phoneInfo }}</b>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>电子邮箱：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt3\" v-model=\"email\"  @blur=\"checkEmail\"  :readonly=\"!emailStatus\" :disabled=\"isDisabled\"/>\n                                <b :class=\"{ 'red-text' : isRed}\" v-if=\"emailStatus\">{{ emailInfo }}</b>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"clear\"></div>\n                <div class=\"JlBoxCon\">\n                    <h1><span>求职意向</span>\n                        <div></div>\n                    </h1>\n                    <div class=\"JlBoxLeft\">\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>求职状态：</div>\n                            <div class=\"inputtxt\">\n                                <select class=\"txtcon txt2\" v-model=\"jobStatus\" :disabled=\"isDisabled\">\n                                    <option value=\"0\">离职-随时到岗</option>\n                                    <option value=\"1\">在职-月内到岗</option>\n                                    <option value=\"2\">在职-考虑机会</option>\n                                    <option value=\"3\">在职-暂不考虑</option>\n                                </select>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>相关工作经验：</div>\n                            <div class=\"inputtxt\">\n                                <label for=\"have\">\n                                    <div class=\"typeJob\" :class=\"{ on: isActive }\" @click=\"showJob('yes')\" v-if=\"showHave\" >有</div>\n                                </label>\n                                <input type=\"radio\" id=\"have\" value=\"1\" v-model=\"workExp\" name=\"man\" v-show=\"false\" >\n\n                                <label for=\"not-have\">\n                                    <div class=\"typeJob\" :class=\"{ on: !isActive }\" @click=\"showJob('no')\" v-if=\"showNot\">无</div>\n                                </label>\n                                <input type=\"radio\" id=\"not-have\" value=\"0\" v-model=\"workExp\" name=\"man\" v-show=\"false\">\n                                <div class=\"clear\"></div>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>期望工作性质：</div>\n                            <div class=\"inputtxt\">\n                                <label for=\"full\">\n                                    <div class=\"typeJob\" :class=\"{ on: show1 }\" @click=\"showThreeJob(1)\" v-if=\"showFull\">全职</div>\n                                </label>\n                                <input type=\"radio\" id=\"full\" value=\"0\" v-model=\"wantedNature\" name=\"man\" v-show=\"false\">\n\n                                <label for=\"part\">\n                                    <div class=\"typeJob\" :class=\"{ on: show2 }\" @click=\"showThreeJob(2)\" v-if=\"showPart\">兼职</div>\n                                </label>\n                                <input type=\"radio\" id=\"part\" value=\"1\" v-model=\"wantedNature\" name=\"man\" v-show=\"false\">\n\n                                <label for=\"practice\">\n                                    <div class=\"typeJob\" :class=\"{ on: show3 }\" @click=\"showThreeJob(3) \" v-if=\"showPractice\">实习</div>\n                                </label>\n                                <input type=\"radio\" id=\"practice\" value=\"2\" v-model=\"wantedNature\" name=\"man\"\n                                    v-show=\"false\">\n                                <div class=\"clear\"></div>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>期望月薪：</div>\n                            <div class=\"inputtxt\">\n                                <select class=\"txtcon txt2\" v-model=\"wantedSalary\" :disabled=\"isDisabled\">\n                                    <option value=\"0\">面议</option>\n                                    <option value=\"5\">30000以上</option>\n                                    <option value=\"4\">20000-30000</option>\n                                    <option value=\"3\">10000-20000</option>\n                                    <option value=\"2\">5000-10000</option>\n                                    <option value=\"1\">5000以下</option>\n                                </select>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>期望工作地点：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt7\" v-model=\"wantedLocation\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>期望职位类别：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt7\" v-model=\"wantedType\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <button id=\"next_btn\" class=\"nextBtn\" @click=\"show()\">进一步完善</button>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div id=\"education_div\" v-show=\"show_education\" class=\"perRightcon\">\n                <div class=\"JlBoxTit\">\n                    <p class=\"tit1\">基本资料/求职意向</p>\n                    <div class=\"tit2\"></div>\n                    <p class=\"tit3 on\">最高学历</p>\n                </div>\n                <div class=\"JlBoxCon\">\n                    <h1><span>最高学历</span>\n                        <div></div>\n                    </h1>\n                    <div class=\"JlBoxLeft\">\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>学校名称：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt3\" v-model=\"schoolName\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>毕业时间：</div>\n                            <div class=\"inputtxt\">\n                                <input id=\"graduation_time\" name=\"\" type=\"date\" class=\"txt8\" v-model=\"graduationTime\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>学历：</div>\n                            <div class=\"inputtxt\">\n                                <select class=\"txtcon txt2\" v-model=\"degree\" :disabled=\"isDisabled\">\n                                    <option value=\"0\">高中</option>\n                                    <option value=\"1\">专科</option>\n                                    <option value=\"2\">本科</option>\n                                    <option value=\"3\">硕士</option>\n                                    <option value=\"4\">博士</option>\n                                </select>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>学历类型：</div>\n                            <div class=\"inputtxt\">\n                                <select class=\"txtcon txt2\" v-model=\"degreeType\" :disabled=\"isDisabled\">\n                                    <option value=\"0\">普通高等教育（统招）</option>\n                                    <option value=\"1\">普通专升本（统招）</option>\n                                    <option value=\"2\">成人高考</option>\n                                    <option value=\"3\">自考</option>\n                                    <option value=\"4\">电大</option>\n                                    <option value=\"5\">网络教育</option>\n                                </select>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>专业名称：</div>\n                            <div class=\"inputtxt\">\n                                <input name=\"\" type=\"text\" class=\"txt3\" v-model=\"majorName\" :disabled=\"isDisabled\"/>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <div class=\"nametxt\"><span>*</span>专业类别：</div>\n                            <div class=\"inputtxt\">\n                                <select class=\"txtcon txt2\" v-model=\"majorType\" :disabled=\"isDisabled\">\n                                    <option value=\"0\">哲学</option>\n                                    <option value=\"1\">经济学</option>\n                                    <option value=\"2\">法学</option>\n                                    <option value=\"3\">教育学</option>\n                                    <option value=\"4\">文学</option>\n                                    <option value=\"5\">历史学</option>\n                                    <option value=\"6\">理学</option>\n                                    <option value=\"7\">工学</option>\n                                    <option value=\"8\">农学</option>\n                                    <option value=\"9\">医学</option>\n                                    <option value=\"10\">军事学</option>\n                                    <option value=\"11\">管理学</option>\n                                    <option value=\"12\">艺术学</option>\n                                </select>\n                            </div>\n                        </div>\n                        <div class=\"clear\"></div>\n                        <div class=\"jldiv\">\n                            <button class=\"saveBtn\" @click=\"addUserInfo\" v-show=\"!isDisabled\">填写完毕</button>\n                            <button id=\"previous_btn\" class=\"preBtn\" @click=\"show()\" style=\"\">上一步</button>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"clear\"></div>\n        <div class=\"footer\"></div>\n    </div>\n</template>\n<script>\nimport axios from 'axios';\nimport Cookies from 'js-cookie';\nexport default {\n    data() {\n        return {\n            show_base: true,\n            show_education: false,\n            show_sex: true,\n            isActive: true,\n            show1: true,\n            show2: false,\n            show3: false,\n            trueName: \"\",//姓名\n            gender: 1,//性别\n            birthday: \"\",//出生日期\n            resident: \"\",//现居住地\n            place: \"\",//户口所在地\n            growPlace: \"\",//成长所在地\n            phone: \"\",//手机号码\n            email: \"\",//电子邮件\n            jobStatus: 0,//求职状态\n            workExp: 1,//工作经验\n            wantedNature: 0,//期望工作性质\n            wantedSalary: 0,//期望月薪\n            wantedLocation: \"\",//期望工作地点\n            wantedType: \"\",//期望职位类别\n            schoolName: \"\",//学校名称\n            graduationTime: \"\",//毕业时间\n            degree: 0,//学历\n            degreeType: 0,//学历类型\n            majorName: \"\",//专业名称\n            majorType: 0,//专业类别\n            /* 状态中false为只读状态,不显示提示信息 */\n            phoneStatus: false,\n            phoneInfo:\"可用来登录\",\n            emailStatus: false,\n            emailInfo:\"可用来登录\",\n            isRed: false,\n            isDisabled:false,\n            showMan:false,\n            showWoman:false,\n            showHave:false,\n            showNot:false,\n            showFull:false,\n            showPart:false,\n            showPractice:false\n        }\n    },\n    methods: {\n        show() {\n            this.show_base = !this.show_base\n            this.show_education = !this.show_education\n        },\n        showSex(sex) {\n            if (sex == 'man') {\n                this.show_sex = true\n            } else {\n                this.show_sex = false\n            }\n        },\n        showJob(status) {\n            if (status == 'yes') {\n                this.isActive = true\n            } else {\n                this.isActive = false\n            }\n        },\n        showThreeJob(i) {\n            this.show1 = false,\n                this.show2 = false,\n                this.show3 = false\n            if (i == 1) {\n                this.show1 = true\n            }\n            if (i == 2) {\n                this.show2 = true\n            }\n            if (i == 3) {\n                this.show3 = true\n            }\n        },\n        /* 数据回显 */\n        echoData(){\n            let that = this;\n            const cookieValue = Cookies.get('cookieUserId');\n            axios({\n                method: 'GET',\n                url: '/api/user-info/getUserInfo/'+cookieValue\n            })\n                .then(response => {\n                    let data = response.data\n                    let code = data.code\n                    let msg = data.msg\n                    let  info = data.object\n                    if (code === 200001) {\n                        /* 从用户信息表获取到数据的话进行数据回显 */\n                        console.log(data)\n                        this.trueName =  info.name\n                        const backendDate = new Date(info.birthday); // 使用ISO 8601日期字符串创建Date对象\n                        backendDate.setDate(backendDate.getDate() + 1);\n                        this.birthday = backendDate.toISOString().split('T')[0];\n                        this.resident = info.resident\n                        this.place =  info.place\n                        this.growPlace =  info.growPlace\n                        this.phone = info.phone\n                        this.email = info.email\n                        this.wantedLocation = info.wantedLocation\n                        this.wantedType = info.wantedType\n                        this.schoolName = info.schoolName\n                        this.majorName = info.majorName \n                        const  backendDate2 = new Date(info.graduationTime); // 使用ISO 8601日期字符串创建Date对象\n                        backendDate2.setDate(backendDate2.getDate() + 1);\n                        this.graduationTime = backendDate2.toISOString().split('T')[0];\n                        this.isDisabled = true\n                        /* 多选框显示 */\n                        if(info.gender == 0){\n                            this.showMan = false\n                            this.showWoman = true\n                            this.show_sex = false\n                        }else{\n                            this.showWoman = false\n                            this.showMan = true\n                            this.show_sex = true\n                        }\n                        if(info.workExp == 0){\n                            this.showNot = true\n                            this.showHave = false\n                            this.isActive = false\n                        }else{\n                            this.showNot = false\n                            this.showHave = true\n                            this.isActive = true\n                        }\n\n                        this.show1 = false,\n                        this.show2 = false,\n                        this.show3 = false\n                        if (info.wantedNature == 0) {\n                            this.show1 = true\n                            this.showFull = true\n                            this.showPart = false\n                            this.showPractice = false\n                        }\n                        if (info.wantedNature == 1) {\n                            this.show2 = true\n                            this.showFull = false\n                            this.showPart = true\n                            this.showPractice = false\n                        }\n                        if (info.wantedNature == 2) {\n                            this.show3 = true\n                            this.showFull = false\n                            this.showPart = false\n                            this.showPractice = true\n                        }\n                    } else {\n                        console.log(msg)\n                        this.showNot = true\n                        this.showHave = true\n                        this.showMan = true\n                        this.showWoman = true\n                        this.showFull = true\n                        this.showPart = true\n                        this.showPractice = true\n                        that.getPhoneOrEmail();\n                    }\n                }, error => {\n                    console.log('错误', error.message)\n                })\n        },\n        getPhoneOrEmail() {\n            let that = this;\n            let cookieValue = Cookies.get('cookieUserId');\n            axios({\n                method: 'GET',\n                url: '/api/user-info/getPhoneOrEmail/'+cookieValue //你的后端路径\n            })\n                .then(response => {\n                    let data = response.data\n                    let code = data.code\n                    let msg = data.msg\n                    if (code === 200001) { //判断你的请求是否成功\n                        console.log(data)\n                        if (data.object.phone == \"\") {\n                            that.email = data.object.email\n                            that.phoneStatus = true\n                        } else {\n                            that.phone = data.object.phone\n                            that.emailStatus = true\n                        }\n                    } else {\n                        alert(msg)\n                    }\n                }, error => {\n                    console.log('错误', error.message)\n                    // alert(error.message)\n                })\n        },\n        addUserInfo() {\n            let that = this;\n            let cookieValue = Cookies.get('cookieUserId');\n            if (\n                this.trueName != \"\" \n                && this.birthday != \"\"\n                && this.resident != \"\"\n                && this.place !=  \"\"\n                && this.growPlace !=  \"\"\n                && this.phone != \"\"\n                && this.email != \"\"\n                && this.wantedLocation !=  \"\"\n                && this.wantedType != \"\"\n                && this.schoolName != \"\"\n                && this.majorName != \"\" \n                ) {\n                    /* 如果信息填写完整则进行请求发送 */\n                    axios({\n                    method: 'post',\n                    url: '/api/user-info',\n                    data: {\n                        userId: cookieValue,\n                        name: that.trueName,\n                        gender: that.gender,\n                        birthday: that.birthday,\n                        resident: that.resident,\n                        place: that.place,\n                        growPlace: that.growPlace,\n                        phone: that.phone,\n                        email: that.email,\n                        jobStatus: that.jobStatus,\n                        workExp: that.workExp,\n                        wantedNature: that.wantedNature,\n                        wantedSalary: that.wantedSalary,\n                        wantedLocation: that.wantedLocation,\n                        wantedType: that.wantedType,\n                        schoolName: that.schoolName,\n                        graduationTime: that.graduationTime,\n                        degree: that.degree,\n                        degreeType: that.degreeType,\n                        majorName: that.majorName,\n                        majorType: that.majorType,\n                    }\n                })\n                .then(response => {\n                    let data = response.data\n                    let code = data.code\n                    let msg = data.msg\n                    if (code === 300001) {\n                        /* 进行数据回显 */\n                        console.log(data.object)\n                        alert(msg)\n                       this.echoData()\n                        this.show_base = !this.show_base\n                        this.show_education = !this.show_education\n                    } else {\n                        alert(msg)\n                    }\n                }, error => {\n                    console.log('错误', error.message)\n                    // alert(error.message)\n                })\n            }else{\n                alert(\"请填写完整信息\")\n            }\n        },\n        /* 表单校验 */\n        checkPhone(){\n            let phone_REG = /^(0\\d{2,3}-\\d{7,8}(-\\d{1,6})?|[1-9]\\d{6,7}(-\\d{1,6})?|1[3-9]\\d{9})$/;\n            if (phone_REG.test(this.phone)) {\n                this.isRed = false;\n                this.phoneInfo = \"可用于登录\"\n            }else{\n                this.isRed = true;\n                this.phoneInfo = \"手机格式不正确\"\n            }\n        },\n        checkEmail(){\n            let email_REG = /^[a-zA-Z0-9]+@[a-zA-Z0-9]+\\.(com|cn)$/;\n            if (email_REG.test(this.email)) {\n                this.isRed = false;\n                this.emailInfo = \"可用于登录\"\n            }else{\n                this.isRed = true;\n                this.emailInfo = \"邮箱格式不正确\"\n            }\n        }\n    },\n    mounted() {\n        this.echoData();\n\n    }\n}\n</script>"],"mappings":";;;;;AAwQA,OAAAA,KAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,SAAA;MACAC,cAAA;MACAC,QAAA;MACAC,QAAA;MACAC,KAAA;MACAC,KAAA;MACAC,KAAA;MACAC,QAAA;MAAA;MACAC,MAAA;MAAA;MACAC,QAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MAAA;MACAC,SAAA;MAAA;MACAC,KAAA;MAAA;MACAC,KAAA;MAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,YAAA;MAAA;MACAC,YAAA;MAAA;MACAC,cAAA;MAAA;MACAC,UAAA;MAAA;MACAC,UAAA;MAAA;MACAC,cAAA;MAAA;MACAC,MAAA;MAAA;MACAC,UAAA;MAAA;MACAC,SAAA;MAAA;MACAC,SAAA;MAAA;MACA;MACAC,WAAA;MACAC,SAAA;MACAC,WAAA;MACAC,SAAA;MACAC,KAAA;MACAC,UAAA;MACAC,OAAA;MACAC,SAAA;MACAC,QAAA;MA